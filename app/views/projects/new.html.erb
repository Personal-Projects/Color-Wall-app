<br>
<h1 class="project_form">New Project</h1>
<%= form_tag '/projects', method: :post do  %>
  <div class="project_form">
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>
  <br>
  <div class="project_form">
    <%= label_tag :start_date %>
    <%= text_field_tag :start_date %>
  </div>
  <br>
  <div class="project_form">
    <%= label_tag :finish_date %>
    <%= text_field_tag :finish_date %>
  </div>
  <br>
  <div class="project_form">
    <%= label_tag :concept %>
    <%= text_field_tag :concept %>
  </div>
  <br>
  <div class= "project_form" class="form-group"><input type="submit" value="Create Project" class="btn btn-default"></div>
<% end %>

